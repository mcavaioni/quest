<table class="table table-striped table-hover">
  <th> Questionnaire ID </th>
  <th>Name</th>
  <th>Action</th>
  <% @questionnaires.each do |questionnaire| %>
      <tr>
        <td> </td>
        <td><%=link_to questionnaire.name, questionnaire_path(questionnaire)  %></td>
        <td>
          <%=link_to 'edit', edit_questionnaire_path(questionnaire) %> 
          <%=link_to 'delete', questionnaire_path(questionnaire), method: :delete, data: {confirm: "Are you sure you want to delete?"} %>
        </td>
      </tr>
    <%end%>
</table>